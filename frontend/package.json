{
    "name": "where-dem-dollars-at",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@material-ui/core": "^4.11.0",
        "@material-ui/icons": "^4.9.1",
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.3.2",
        "@testing-library/user-event": "^7.1.2",
        "@types/graphql": "^14.5.0",
        "@types/jest": "^24.0.0",
        "@types/react": "^16.9.0",
        "@types/react-dom": "^16.9.0",
        "@types/react-router-dom": "^5.1.6",
        "apollo": "^2.33.2",
        "clsx": "^1.1.1",
        "graphql": "^15.5.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-router-dom": "^5.2.0",
        "react-scripts": "3.4.3",
        "typescript": "4.2.4",
        "xlsx": "^0.17.0"
    },
    "scripts": {
        "start": "NODE_ENV=development react-scripts start",
        "build": "NODE_ENV=production react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "download-schema": "npx apollo service:download --endpoint=http://localhost:8080/graphql graphql-schema.json",
        "generate": "yarn run download-schema && apollo codegen:generate --localSchemaFile=graphql-schema.json --target=typescript --tagName=gql",
        "codegen": "yarn graphql-codegen --config codegen.yml && yarn generate"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.11.4",
        "@graphql-codegen/cli": "1.21.4",
        "@graphql-codegen/introspection": "1.18.2",
        "@graphql-codegen/typescript": "1.22.0",
        "@graphql-codegen/typescript-operations": "1.17.16",
        "@graphql-codegen/typescript-react-apollo": "2.2.4",
        "babel-loader": "^8.1.0",
        "fork-ts-checker-webpack-plugin": "^5.1.0",
        "http-proxy-middleware": "^2.0.0",
        "prettier": "^2.3.0",
        "react-is": "^16.13.1",
        "ts-loader": "^8.0.2"
    }
}
